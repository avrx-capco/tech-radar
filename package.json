{
  "name": "capco-radar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cucumber/cucumber": "^8.10.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "@mui/styled-engine-sc": "^5.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.16.16",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.2.4",
    "dotenv": "^16.0.3",
    "jest-styled-components": "^7.1.1",
    "playwright": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "cucumberTest": "./node_modules/.bin/cucumber-js --publish --exit",
    "coverage": "nyc npm run cucumberTest",
    "cypress": "cypress open",
    "eject": "react-scripts eject",
    "type-check": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.js \"src/**/*.{json,ts,tsx}\"",
    "lint:fix": "eslint -c .eslintrc.js --fix \"src/**/*.{json,ts,tsx}\"",
    "prepare": "husky install"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!d3|d3-array|internmap|delaunator|robust-predicates)"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "prettier --write",
      "npm run lint:fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react-hooks/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chroma-js": "^2.1.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cypress": "^12.7.0",
    "d3": "^7.8.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest-mock-axios": "^4.7.0-beta4",
    "lint-staged": "^13.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "react-scripts": "^5.0.1"
  }
}
